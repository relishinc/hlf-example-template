{
  "defaultSeverity": "error",
  "skipNullChecks": "false",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-console": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "one-line": [
      true,
      "check-open-brace"
    ],
    "prefer-for-of": false,
    "prefer-readonly": {
      "severity": "warning"
    },
    "no-unused-expression": {
      "severity": "warning"
    },
    "no-trailing-whitespace": {
      "severity": "warning"
    }
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "src/scripts/vendor/**",
      "src/scripts/Data/Version.ts"
    ]
  }
}
